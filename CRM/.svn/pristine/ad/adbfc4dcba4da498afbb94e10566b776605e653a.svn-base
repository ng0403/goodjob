package com.crm.cp.standard.data_board.vo;

import java.sql.Date;

import org.springframework.stereotype.Component;

@Component
public class Data_boardVO {
	
	String data_board_id;
	String org_id;
	String data_div_cd;
	String title;
	String cont;
	String file_yn;
	String fst_reg_id_nm;
	Date fst_reg_dt;
	String fin_mdfy_id_nm;
	Date fin_mdfy_dt;
	String act_yn;
	int dboardidx;

	public Data_boardVO(){}

	public Data_boardVO(String data_board_id, String org_id,
			String data_div_cd, String title, String cont, String file_yn,
			String fst_reg_id_nm, Date fst_reg_dt, String fin_mdfy_id_nm,
			Date fin_mdfy_dt, String act_yn) {
		super();
		this.data_board_id = data_board_id;
		this.org_id = org_id;
		this.data_div_cd = data_div_cd;
		this.title = title;
		this.cont = cont;
		this.file_yn = file_yn;
		this.fst_reg_id_nm = fst_reg_id_nm;
		this.fst_reg_dt = fst_reg_dt;
		this.fin_mdfy_id_nm = fin_mdfy_id_nm;
		this.fin_mdfy_dt = fin_mdfy_dt;
		this.act_yn = act_yn;
	}

	public String getData_board_id() {
		return data_board_id;
	}

	public void setData_board_id(String data_board_id) {
		this.data_board_id = data_board_id;
	}

	public String getOrg_id() {
		return org_id;
	}

	public void setOrg_id(String org_id) {
		this.org_id = org_id;
	}

	public String getData_div_cd() {
		return data_div_cd;
	}

	public void setData_div_cd(String data_div_cd) {
		this.data_div_cd = data_div_cd;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getCont() {
		return cont;
	}

	public void setCont(String cont) {
		this.cont = cont;
	}

	public String getFile_yn() {
		return file_yn;
	}

	public void setFile_yn(String file_yn) {
		this.file_yn = file_yn;
	}

	public String getFst_reg_id_nm() {
		return fst_reg_id_nm;
	}

	public void setFst_reg_id_nm(String fst_reg_id_nm) {
		this.fst_reg_id_nm = fst_reg_id_nm;
	}

	public Date getFst_reg_dt() {
		return fst_reg_dt;
	}

	public void setFst_reg_dt(Date fst_reg_dt) {
		this.fst_reg_dt = fst_reg_dt;
	}

	public String getFin_mdfy_id_nm() {
		return fin_mdfy_id_nm;
	}

	public void setFin_mdfy_id_nm(String fin_mdfy_id_nm) {
		this.fin_mdfy_id_nm = fin_mdfy_id_nm;
	}

	public Date getFin_mdfy_dt() {
		return fin_mdfy_dt;
	}

	public void setFin_mdfy_dt(Date fin_mdfy_dt) {
		this.fin_mdfy_dt = fin_mdfy_dt;
	}

	public String getAct_yn() {
		return act_yn;
	}

	public void setAct_yn(String act_yn) {
		this.act_yn = act_yn;
	}
	public int getDboardidx() {
		return dboardidx;
	}

	public void setDboardidx(int dboardidx) {
		this.dboardidx = dboardidx;
	}

}
