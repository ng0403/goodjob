@CHARSET "UTF-8";
/*기본값*/
html{
	font-family: 'Nanum Gothic', Dotum, "돋움", Gulim, "굴림", Verdana, Arial, "Trebuchet MS";
	border-collapse: collapse;
	font-size: 13px;
}

#title .caption{
  	color:#484848;
  	font-weight:bold;
  	border-bottom: 1px solid #dddddd;
  	padding-bottom: 10px;
  }
  
#selectcode{
	background-color: #ffffff;
	color: #5F5F5F;
	border: 1px solid #C0C4CB;
	padding: 4px 4px;
	border-radius: 5px 5px 5px 5px;
	width: 130px;
	margin-top: 7px;
}

#dboardbody{
	margin-left: 5%;
	margin-right: 5%;
}

.titleinput{
	margin-top: 7px;
	width: 100%;
	border: 1px solid #dddddd;
	height: 25px;
	padding-left: 5px;
} 

#cont{
	height: 410px;
	margin-top: 5px;
	padding-top: 5px;
	padding-left: 5px;
	float: left;
	width: 100%;
	border: 1px solid #dddddd;
	overflow-y: scroll;
}

.btn {
	display: inline-block;
	padding: 4px 9px;
	margin-bottom: 0;
	font-weight: normal;
	line-height: 1.428571429;
	text-align: center;
	white-space: nowrap;
	vertical-align: middle;
	cursor: pointer;
	border: 1px solid transparent;
	border-radius: 4px;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	-o-user-select: none;
	user-select: none;
	color: #353535;
	background-color: #EAEAEA;
	border-color: #BDBDBD;
}

.btn-default:hover{
	color: #ffffff;
	background-color: #6B6B6B;
	border-color: #BDBDBD;
}

#btndiv{
	margin-top: 7px;
	float: right;
}
  
#filediv{
	
	float:left;
	width: 99.5%;
	margin-top: 7px;
	border: 1px solid #dddddd;
	padding: 5px;
	height: 15px;
	overflow-y: scroll; 
	
}

.filedivtext{
	color: #BDBDBD;
}

#filedata{
	visibility: hidden;
}

#filetextfield{
	width: 100%;
	height: 100%;
	border: 0px;
}

#filetextfield[disabled]{
	background-color: white;
}