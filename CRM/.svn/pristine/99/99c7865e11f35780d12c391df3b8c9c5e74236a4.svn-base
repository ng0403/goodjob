@CHARSET "UTF-8";
html{
	font-family: 'Nanum Gothic', Dotum, "돋움", Gulim, "굴림", Verdana, Arial, "Trebuchet MS";
	border-collapse: collapse;
    border-spacing: 0;
    font-size: 13px;
}

select{
	 margin-left: 10px;
  	 background-color:#ffffff;
 	 color:#5F5F5F;
 	 border:1px solid #C0C4CB;
 	 padding:5px 5px;
 	 border-radius:5px 5px 5px 5px;
 	 width: auto;
 	/*  margin-right: 30px; */
}

#title .caption{
  	/* margin-left: 0px; */
  	color:#484848;
  	font-weight:bold;
  	border-bottom: 1px solid #dddddd;
  	padding-bottom: 10px;
  }
  
.int_search{
	position:relative;
	width:40%;
	height:24px;
	border:1px solid #bebebe;
	color:#232323;
	border-radius: 5px 5px 5px 5px;
}  
/* -----------------------------------------권한관리리스트----------------------------------------------- */
.auth_bt_position{
	padding-top: 10px;
	float: right;
}

.search_bt_position{
	padding-top: 10px;
	padding-bottom: 10px;
	float: right;
	
}

.bs-example #authtable{
	width:100%;
	border-collapse: collapse;
    border-spacing: 0;
    border: 1px solid #dddddd;
    
}

.bs-example #authtable thead{
	float: left; width:100%;
}

.bs-example #authtable thead tr{
	height:35px;
}

.bs-example #authtable thead td{
	font-weight: bold;
	vertical-align: bottom;
    border-bottom: 2px solid #dddddd;
    padding: 8px;
}

.bs-example #authtable thead td,
.bs-example #authtable thead tr{
	background-color: #F2F2F2;
	
}

.bs-example #authtable  tbody tr:hover{
	background-color:#ebe7e7;
}

.bs-example #authtable  #userAuthBody tr:hover{
	background-color:#ebe7e7;
}

.bs-example_orglist #mastertable #userAuthBody tr:hover{
	background-color:#ebe7e7;
}

.bs-example_orglist #mastertable tbody tr:hover{
	background-color:#ebe7e7;
}

.bs-example #authtable  tbody{
	overflow-y:scroll;
	overflow-x:hidden;
	float: left;
	width: 100%;
	height: 400px;
}
.bs-example #authtable  tbody tr{
	display: table;
	width: 100%;

}

.bs-example #authtable  tbody td{
/* 	width: 25%; */
	padding: 8px;
    line-height: 1.428571429;
    vertical-align: top;
    border-top: 1px solid #dddddd;
    padding-left:20px;
    text-align: left;
}

.bs-example #authtable  thead tr th{
  vertical-align: center;
  border-bottom: 2px solid #dddddd;
}

